{% extends '@layout/default.html.twig' %}

{% block title %}Visualizar Foto{% endblock %}

{% block content %}
    <div class="row">
        {% for image in response.data %}
            <div class="col-md-6">
                <h2>{{ image.slug }}</h2>
                <img class="col-md-12 img-responsive pull-left" src="{{ image.images.fixed_height.url }}">
            </div>
        {% endfor %}
    </div>
{% endblock %}
